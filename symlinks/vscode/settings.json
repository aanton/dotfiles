{
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client",
    "editor.tabSize": 4
  },
  "cSpell.language": "en,es-ES",
  "codesnap.backgroundColor": "#81A1C1",
  "codesnap.containerPadding": "2em",
  "debug.allowBreakpointsEverywhere": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.emptySelectionClipboard": false,
  "editor.foldingImportsByDefault": true,
  "editor.fontFamily": "JetBrains Mono, Fira Code, Ubuntu Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "editor.guides.bracketPairs": true,
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [80, 120],
  "editor.suggest.localityBonus": true,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
  "editor.wordWrap": "on",
  "explorer.openEditors.visible": 0,
  "files.defaultLanguage": "markdown",
  "git.mergeEditor": false,
  "gitlens.keymap": "chorded",
  "gitlens.views.repositories.files.layout": "tree",
  "gitlens.views.searchAndCompare.files.layout": "list",
  "keyboard.dispatch": "keyCode",
  "markdown-preview-enhanced.enableScriptExecution": true,
  "material-icon-theme.hidesExplorerArrows": true,
  "open-in-browser.default": "google-chrome",
  "php.suggest.basic": false,
  "php.validate.executablePath": "/usr/bin/php",
  "php.validate.run": "onType",
  "prettier.requireConfig": true,
  "screencastMode.fontSize": 40,
  "screencastMode.onlyKeyboardShortcuts": true,
  "screencastMode.verticalOffset": 0,
  "template-string-converter.autoRemoveTemplateString": true,
  "terminal.integrated.fontFamily": "UbuntuMono Nerd Font Mono",
  "typescript.workspaceSymbols.scope": "currentProject",
  "window.newWindowDimensions": "inherit",
  "window.title": "${dirty}${rootName}${separator}${activeEditorMedium}",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 1,
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "Nord",
  "workbench.editor.enablePreview": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.showTabs": false,
  "workbench.editor.untitled.hint": "hidden",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.list.keyboardNavigation": "filter",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "newUntitledFile"
}
